<article class="event">
    <h1>EVENT</h1>
    <h3>{{event.title}}</h3>
    <h4>{{event.startTime}}</h4>
    <p>{{event.description}}</p>
    <p>{{event.location}}</p>
    <p>{{event.attendees}}</p>
    
    
    <h3>Attendees Name</h3>
      {{#each event.attendeesName}}
        <ul>
        <li><a href="/people/{{this.id}}">{{this.name}}</a></li>
        </ul>       
    {{/each}}

<h3> Location</h3>
 <li><a href="/locations/{{event.locationData.id}}">{{event.locationData.name}}</a></li>

</article>
